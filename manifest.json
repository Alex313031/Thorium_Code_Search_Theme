{
  "content_scripts": [
    {
      "matches": [
        "https://cs.chromium.org/*",
        "https://source.chromium.org/*"
      ], 
      "js": [
        "js/jquery-3.6.0.min.js", 
        "js/generated.js", 
        "js/chromiumcs_content_script.js",
        "js/popup.js"
      ], 
      "css": [
        "css/generated.css",
        "css/popup.css"
      ], 
      "run_at": "document_start"
    }
  ], 
  "description": "Displays Chromium code search in a customized theme.", 
  "short_name": "codesearch-themer", 
  "icons": {
    "128": "themes-icon.png"
  }, 
  "background": {
    "scripts": [
      "js/background.js"
    ]
  }, 
  "name": "Thorium CodeSearch Themer", 
  "page_action": {
    "default_name": "Select Theme Style", 
    "default_icon": "themes-icon.png", 
    "default_popup": "popup.html"
  }, 
  "version": "0.3.0", 
  "manifest_version": 2, 
  "minimum_chrome_version": "48", 
  "permissions": [
    "tabs", 
    "storage", 
    "https://cs.chromium.org/*",
    "https://source.chromium.org/*"
  ]
}
