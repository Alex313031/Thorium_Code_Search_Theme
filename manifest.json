{
  "name": "chromiumcs",
  "version": "0.6.1",
  "description": "Displays chromium code search in different colors",
  "icons": { "128": "icon.png" },
  "background": { "scripts": ["background.js"] },
  "content_scripts": [
    { "matches": ["https://code.google.com/p/chromium/*"], "js": ["chromiumcs_content_script.js"] , "css" : ["chromiumcs_content.css"] }
  ],
  "permissions": [
    "tabs",
    "https://maps.google.com/*",
    "https://maps.googleapis.com/*"
  ],
  "page_action": {
      "default_name": "Select style",
      "default_icon": "marker.png",
      "default_popup": "popup.html"
  },
  "manifest_version": 2
}
